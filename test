  form.appendChild(loginBtn);
  form.appendChild(text1);
  
  loginBtn.addEventListener("click", function(e) {
    e.preventDefault();
    if (input.value === password) {
      alert("Login successful!");
    } else {
      alert("Password " + input.value + " unsuccesfull");
     }



  });

